#X3D V4.0 utf8

PROFILE Core
COMPONENT Scripting : 1

Script {
   url "ecmascript:
async function initialize ()
{
   const
      response = await fetch (new URL ('base64-with-bom.url', Browser .currentScene .worldURL)),
      text     = await response .text ();

   await Browser .loadURL (new MFString (text));
   await Browser .nextFrame ();
   Browser .viewAll (0);
}
   "
}
