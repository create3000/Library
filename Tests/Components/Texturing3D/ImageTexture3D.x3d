          </field>
          <IS>
            <connect nodeField='dimension' protoField='dimension'/>
          </IS>
<![CDATA[vrmlscript:

function initialize ()
{
	set_dimension ();
}

function set_dimension ()
{
	var
		coord    = geometry .coord,
		texCoord = geometry .texCoord;
	
	coord    .point .length = 0;
	texCoord .point .length = 0;

	for (var i = 0; i < dimension; ++ i)
	{
		var
			depth = i / (dimension - 1),
			z     = 0.5 - depth;

		coord .point .push (new SFVec3f ( 0.5,  0.5, z),
		                    new SFVec3f (-0.5,  0.5, z),
		                    new SFVec3f (-0.5, -0.5, z),
		                    new SFVec3f ( 0.5, -0.5, z));

		texCoord .point .push (new SFVec3f (1, 1, depth),
		                       new SFVec3f (0, 1, depth),
		                       new SFVec3f (0, 0, depth),
		                       new SFVec3f (1, 0, depth));
	}
}
]]>
        </Script>
