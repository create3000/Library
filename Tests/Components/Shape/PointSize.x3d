<![CDATA[ecmascript:

function initialize ()
{
   group .children .length = 0;

   for (let i = 0; i < 101; ++ i)
   {
      build (i / 10);
   }
}

const colors = [
   new SFColor (0, 1, 0),
   new SFColor (1, 1, 1),
   new SFColor (1, 0, 0),
   new SFColor (0, 0, 1),
   new SFColor (1, 0, 1),
   new SFColor (0, 1, 1),
   new SFColor (1, 1, 0),
   new SFColor (0, 1, 0),
   new SFColor (1, 1, 1),
   new SFColor (1, 0, 0),
   new SFColor (0, 0, 1),
   new SFColor (1, 0, 1),
   new SFColor (0, 1, 1),
   new SFColor (1, 1, 0),
];

function build (pointSize)
{
   const
      scene           = Browser .currentScene,
      transform       = scene .createNode ("Transform"),
      shape           = scene .createNode ("Shape"),
      appearance      = scene .createNode ("Appearance"),
      pointProperties = scene .createNode ("PointProperties"),
      geometry        = scene .createNode ("PointSet"),
      color           = scene .createNode ("Color"),
      coord           = scene .createNode ("Coordinate");

   group .children .push (transform);

   transform .children [0]     = shape;
   shape .appearance           = appearance;
   shape .geometry             = geometry;
   appearance .pointProperties = pointProperties;
   geometry .color             = color;
   geometry .coord             = coord;

   transform .translation                = new SFVec3f (pointSize - 5, 0, 0);
   pointProperties .pointSizeScaleFactor = pointSize;
   pointProperties .pointSizeMinValue    = pointSize;
   pointProperties .pointSizeMaxValue    = pointSize;
   color .color [0]                      = colors [Math .floor (pointSize)];
   coord .point [0]                      = new SFVec3f ();
}
